{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/calculate_xirr",
      "dest": "/calculate_xirr"
    },
    {
      "src": "/calculate_irr",
      "dest": "/calculate_irr"
    },
    {
      "src": "/uc_calculate_irr",
      "dest": "/uc_calculate_irr"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
